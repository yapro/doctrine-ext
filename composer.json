{
  "name": "yapro/doctrine-ext",
  "type": "library",
  "description": "Doctrine extensions",
  "keywords": ["php", "doctrine", "extension"],
  "license": "proprietary",
  "authors": [
    {
      "name": "Lebedenko Nikolay",
      "email": "totx@ya.ru",
      "role": "Developer",
      "homepage": "http://yapro.ru"
    }
  ],
  "require": {
    "php": ">=8.0",
    "doctrine/orm": "*",
    "yapro/helpers": "*"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "doctrine/annotations": "*",
    "symfony/cache": "*",
    "symfony/serializer": "*",
    "symfony/phpunit-bridge": "*",
    "phpmd/phpmd" : "@stable"
  },
  "autoload": {
    "psr-4": {
      "YaPro\\DoctrineExt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YaPro\\DoctrineExt\\Tests\\": "tests/"
    }
  }
}
